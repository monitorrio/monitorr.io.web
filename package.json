  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js --bail --progress --profile",
    "build-staging": "rimraf dist && cross-env NODE_ENV=staging webpack --config webpack.config.js --bail --progress --profile",
    "build-production": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --bail --progress --profile",
    "dev": "cross-env NODE_ENV=development node server/server",
    "stage": "cross-env NODE_ENV=staging node server/server",
    "prod": "cross-env NODE_ENV=production node server/server",
    "start": "node server/server",
    "heroku-postbuild": "npm run build",
    "dev-webpack": "node node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "keywords": [
    "dashbard",
    "bootstrap"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
